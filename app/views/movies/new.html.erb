<h1>Add a Movie</h1>
<p>Movie title and year are required fields</p>

<%= form_for(@movie) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :year %>
  <%= f.text_field :year %>
  
  <%= f.label :director %>
  <%= f.text_field :director %>

  <%= f.label :image %>
  <%= f.text_field :image, value: "" %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>
<% end %>

